programa
{
	
	funcao inicio()
	{
		inteiro num[3][3] = {
			{1,3,5},
			{7,9,11},
			{13,15,17}
		}
	inteiro soma = 0
		para(inteiro i=0; i < 3; i++){
			
			para(inteiro j=0; j < 3; j++){
			escreva(num[i][j]," - ")
			soma = soma + num[i][j] 
			
			}
		}escreva("\n\n A soma dos valores Ã© ", soma)
	}
}
